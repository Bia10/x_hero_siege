<root>
	<styles>
		<include src="file://{resources}/styles/custom_game/xhs_events.css" />
	</styles>

	<scripts>
		<include src="file://{resources}/scripts/custom_game/xhs_events.js" />
	</scripts>

	<Panel hittest="false" class="EventsPanel">
		<Panel id="EventPanel">
			<Button id="Event1" onmouseactivate="HeroImage();" >
				<Label class="Event1_Title" text="Hero Image (x4 stats)" id="EventTitle"/>
			</Button>

			<Button id="Event2" onmouseactivate="SpiritBeast();" >
				<Label class="Event1_Title" text="Spirit Beast" id="EventTitle"/>
			</Button>

			<Button id="Event3" onmouseactivate="FrostInfernal();" >
				<Label class="Event1_Title" text="Frost Infernal" id="EventTitle"/>
			</Button>

			<Button id="Event4" onmouseactivate="AllHeroImages();" >
				<Label class="Event1_Title" text="All Hero Images (x2 stats)" id="EventTitle"/>
			</Button>

			<Button id="Close" onmouseactivate="OnQuit();" >
				<Image src="file://{images}/custom_game/adminmod/close.png" />
			</Button>
		</Panel>
	</Panel>
</root>
