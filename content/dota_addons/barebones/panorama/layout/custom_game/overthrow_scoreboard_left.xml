<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/custom_game/overthrow_scoreboard.css" />
	</styles>
	<scripts>
		<include src="file://{resources}/scripts/custom_game/overthrow_scoreboard.js" />
	</scripts>
	<Panel class="ScoreboardLeft">
		<Panel class="VictoryPanel">
			<Label id="Goal" text="#Goal" />
			<Label id="Timer" text="--:--" />
		</Panel>
		<Panel hittest="false" id="ScoreboardCenter">
			<Label hittest="false" id="ScoreboardCurrentTime" class="MonoNumbersFont" text="" />
		</Panel>
		<Panel id="ScoreboardCreepTime">
			<Label id="CreepGoal" text="#CreepGoal" />
			<Label id="CreepTimer" text="--:--" />
		</Panel>
		<Panel id="ScoreboardIncomingWaveTime">
			<Label id="IncomingWaveGoal" text="#IncomingWaveGoal" />
			<Label id="IncomingWaveTimer" text="--:--" />
		</Panel>
		<Panel id="ScoreboardSpecialEventsTime">
			<Label id="SpecialEventsGoal" text="#SpecialEventsGoal" />
			<Label id="SpecialEventsTimer" text="--:--" />
		</Panel>
	</Panel>
</root>
